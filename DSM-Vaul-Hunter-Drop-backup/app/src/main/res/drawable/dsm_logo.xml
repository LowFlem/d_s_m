    <path
        android:fillColor="@color/dsmSecondary"
        android:pathData="M25,60 A10,10 0 1,1 25,40 A10,10 0 1,1 25,60" />
    <path
        android:fillColor="@color/dsmSecondary"
        android:pathData="M175,60 A10,10 0 1,1 175,40 A10,10 0 1,1 175,60" />
